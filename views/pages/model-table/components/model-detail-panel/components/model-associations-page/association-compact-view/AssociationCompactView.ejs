<%_if( isToMany ){-%>
<%if(isLimitOffset) {-%>
  useEffect(() => {
    //update ref
    rowsPerPageRef.current = rowsPerPage;

    //check strict contention
    if(isOnApiRequestRef.current) { return; }

    //reload
    setDataTrigger(prevDataTrigger => !prevDataTrigger);
  }, [rowsPerPage]);
<%} else if(isCursorBased) {-%>
  useEffect(() => {
